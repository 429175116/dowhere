<!--pages/projectAll/projectAll.wxml-->
<!-- 数据汇总页，最高权限可查看 -->
<view class="timeSel">
  <view bindtap="yearData" class="action" >全年计划与完成</view>
  <view bindtap="monthData">当月计划与完成</view>
</view>
<view class="charts">
  <ec-canvas id="mychart-dom-multi-bar" canvas-id="mychart-multi-bar" ec="{{ ecPie }}"></ec-canvas>
  <ec-canvas id="mychart-dom-multi-bar" canvas-id="mychart-multi-bar" ec="{{ ecBar }}"></ec-canvas>
  <ec-canvas id="mychart-dom-multi-scatter" canvas-id="mychart-multi-scatter" ec="{{ ecScatter }}"></ec-canvas>
</view>
<!-- 项目列表展示 -->
<view class="regionList">
  <block wx:for="{{projectListData}}" wx:for-item="item">
    <view class="regionItme">
      <view class="regionName">
        <view>{{item.name}}</view>
      </view>
      <view class="projectList">
        <block wx:for="{{item.project}}" wx:for-item="items">
          <view data-id="{{items.id}}">{{items.projectName}}</view>
        </block>
      </view>
    </view>
  </block>
</view>