/* pages/projectAll/projectAll.wxss */
.timeSel{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100rpx;
  width: 750rpx;
  border: solid #333;
  border-width: 1rpx 0 1rpx 0;
}

.timeSel view{
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  height: 100rpx;
  width: 375rpx;
}
.action{
  background-color: #999;
}
/* 图表显示块 */
.charts{
  height: auto;
}
/* 图表样式 */
#mychart-dom-multi-bar,
#mychart-dom-multi-scatter{
  width: 750rpx;
  height: 500rpx;
  display:block;
}
/* 项目显示部分 */
.regionList,.regionItme{
  width: 750rpx;
  height: auto;
  z-index: 9999;
}
.regionName{
  width: 750rpx;
  height:60rpx;
  line-height:60rpx;
}
.regionName view{
  float:left;
  font-weight:600;
  width:auto;
  height:60rpx;
  line-height:60rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  margin:0 33rpx;
  color:#333;
  font-size:35rpx;
}
/* 项目列表 */
.projectList{
  width:680rpx;
  margin:0 33rpx;
}
/* 项目名称 */
.projectList view{
  width:auto;
  height:45rpx;
  line-height:45rpx;
  padding:0 20rpx;
  display:inline-block;
  margin:20rpx;
  background-color:#e5ecf2;
  color:#999;
  border-radius:10rpx;
  font-size:30rpx;
}
/* 选自进入项目或产品 */
.selGoPage{
  width: 200rpx !important;
  height: 80rpx !important;
  background-color: #333 !important;
  position: relative !important;
  border-radius: 10rpx !important;
  /* left: 100rpx;
  top: 200rpx; */
  z-index: 99 !important;
}
.goPage{
  display:flex;
  align-items:center;
  justify-content:center;
  font-size: 25rpx;
  width: 200rpx;
  height: 80rpx;
}
.goPage view{
  height: 80rpx;
  width: 100rpx;
  display:flex;
  align-items:center;
  justify-content:center;
  color: #ccc;
}
.arrow{
  position: absolute;
  top: -11rpx;
  left: 85rpx;
  width: 32rpx;
  height: 16rpx;
  background: #58a;
  background-color: #58a;
  background: linear-gradient(135deg, transparent 11rpx, #333 0) top left,
                linear-gradient(-135deg, transparent 11rpx, #333 0) top right;
  background-size: 50% 100%;
  background-repeat: no-repeat;
}
