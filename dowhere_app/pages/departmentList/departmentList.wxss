/* pages/departmentList/departmentList.wxss */
.departmentList {
    padding: 25rpx;
    width: 700rpx;
    height: auto;
}
.item{
    display: flex;
    align-items: center;
    justify-content: space-between;
    width: 700rpx;
    height: 100rpx;
    background-color: #f2f2f2;
    margin-bottom: 20rpx;
    font-size: 35rpx;
}
.name{
    margin-left: 20rpx;
}
.go{
    display: flex;
    align-items: center;
    justify-content: center;
    width: 90rpx;
    height: 45rpx;
    background-color: #78ac47;
    border-radius: 30rpx;
    color: #fff;
    margin-right: 20rpx;
    font-size: 25rpx;
}