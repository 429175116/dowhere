/* pages/setPlan/setPlan.wxss */
page{
  width: 750rpx;
}
.pageTitle{
  width: 710rpx;
  margin: 0 20rpx;
  font-size: 30rpx;
  margin-bottom: 50rpx;
}
.pageTitle text{
  width: 40rpx;
}
.setPlanform{
  width: 710rpx;
  margin: 0 20rpx;
  font-size: 30rpx;
}
.setPlanform label {
  margin: 10rpx 0;
}

input,textarea{
  border: 1rpx solid #999;
  width: 710rpx;
  margin: 10rpx 0;
}
.submit{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 710rpx;
  margin: 0 20rpx;
  font-size: 30rpx;
}
.submit view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 300rpx;
  height: 100rpx;
  border: 1rpx solid #999;
}